<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Security\Domain\Model\Entity\User" table="security_user">
        <unique-constraints>
            <unique-constraint columns="email" name="email"/>
        </unique-constraints>
        <id name="id" type="id" column="id"/>
        <field name="email" column="email" type="email" unique="true"/>
        <field name="password" column="password" type="password"/>
        <field name="status" column="status" type="status"/>
    </entity>
</doctrine-mapping>